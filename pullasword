	end)
tgls:Toggle("Auto Buy Potion",false, function(bool)
      if bool then
         -- Start the loop
         _G.loop4 = true
			while _G.loop4 do
				local args = {"Buy",2,1}
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Events"):WaitForChild("OnStandartShopRequest"):FireServer(unpack(args))
                local args = {"Buy",2,2}
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Events"):WaitForChild("OnStandartShopRequest"):FireServer(unpack(args))
                local args = {"Buy",2,3}
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Events"):WaitForChild("OnStandartShopRequest"):FireServer(unpack(args))
                local args = {"Buy",2,4}
                game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Events"):WaitForChild("OnStandartShopRequest"):FireServer(unpack(args))
				task.wait(60) -- Adjust the wait time as needed
			end
      else
         -- Stop the loop
         _G.loop4 = false
      end
		print(bool)
	end)
tgls:Toggle("Auto Rebirth and Super Rebirth",false, function(bool)
      if bool then
         -- Start the loop
         _G.loop4 = true
			while _G.loop4 do
				game:GetService("ReplicatedStorage"):WaitForChild("GameClient"):WaitForChild("Events"):WaitForChild("RemoteEvent"):WaitForChild("RebirthEvent"):FireServer()
                game:GetService("ReplicatedStorage"):WaitForChild("GameClient"):WaitForChild("Events"):WaitForChild("RemoteEvent"):WaitForChild("SuperRebirthEvent"):FireServer()
				task.wait() -- Adjust the wait time as needed
			end
      else
         -- Stop the loop
         _G.loop4 = false
      end
		print(bool)
	end)
local tgls = serv:Channel("Mob Boss")
tgls:Toggle("Mob Bos 42",false, function(bool)
      if bool then
         -- Start the loop
         _G.loop4 = true
			while _G.loop4 do
				local args = {1}
				game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Events"):WaitForChild("RemoveC"):FireServer(unpack(args))
				game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Events"):WaitForChild("WinEvent"):FireServer(42)
				local args = {0}
				game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Events"):WaitForChild("RemoveC"):FireServer(unpack(args))
				task.wait(2) -- Adjust the wait time as needed
			end
      else
         -- Stop the loop
         _G.loop4 = false
      end
		print(bool)
	end)
